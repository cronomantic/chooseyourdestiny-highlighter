{
  "name": "chooseyourdestiny-highlighter",
  "publisher": "cronomantic",
  "displayName": "ChooseYourDestiny Syntax Highlighter",
  "description": "Syntax Highlighter for ChooseYourDestiny CYOA creation suit.",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.86.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cronomantic/chooseyourdestiny-highlighter"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cyd",
        "aliases": [
          "ChooseYourDestiny",
          "cyd"
        ],
        "extensions": [
          ".cyd"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cyd",
        "scopeName": "source.cyd",
        "path": "./syntaxes/cyd.tmLanguage.json"
      }
    ]
  }
}
